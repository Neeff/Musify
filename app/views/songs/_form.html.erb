<%= form_with(model: [playlist, song], local: true) do |form| %>


  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :artist %>
    <%= form.text_field :artist %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
